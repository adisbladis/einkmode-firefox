{
  "manifest_version": 2,
  "name": "Toggle Website Colors (Global)",
  "version": "1.1.0",

  "description": "__MSG_extensionDescription__",

  "browser_specific_settings": {
    "gecko": {
      "id": "einkmode@blad.is",
      "strict_min_version": "61.0"
    }
  },

  "icons": {
    "24": "icons/toggleglobalcolors.svg",
    "32": "icons/toggleglobalcolors.svg",
    "50": "icons/toggleglobalcolors.svg"
  },

  "background": {
    "scripts": ["iconupdater.js", "background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/toggleglobalcolors.svg",
    "default_title": "__MSG_button_title__",
    "theme_icons": [{
      "dark": "icons/toggleglobalcolors.svg",
      "light": "icons/toggleglobalcolors-light.svg",
      "size": 19
    }]
  },

  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },

  "commands": {
    "_execute_browser_action": {
    }
  },

  "permissions": [
    "browserSettings",
    "nativeMessaging"
  ],

  "default_locale": "en"
}
